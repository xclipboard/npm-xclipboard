{
  "newrelic": {
    "license_key": "",
    "logging": "info"
  },
  "elasticsearch": {
    "pictograms": {
      "index": "xclipboard",
      "type": "pictograms",
      "mappings": {
        "pictograms": {
          "properties": {
            "category": {
              "type": "string"
            },
            "id": {
              "type": "string"
            },
            "url": {
              "type": "string"
            }
          }
        }
      }
    },
    "clipboards": {
      "index": "xclipboard",
      "type": "clipboards",
      "mappings": {
        "clipboards": {
          "properties": {
            "createdAt": {
              "format": "dateOptionalTime",
              "type": "date"
            },
            "id": {
              "type": "string"
            },
            "pictogram": {
              "type": "string"
            },
            "text": {
              "type": "string"
            }
          }
        }
      }
    }
  },
  "provider": {
    "selected": "elasticsearch",
    "elasticsearch": {
      "hosts": [
        {
          "protocol": "https",
          "host": "host",
          "auth": "login:pass",
          "port": 443,
          "weight": 1
        }
      ],
      "keepAlive": true
    }
  }
}